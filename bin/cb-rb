#! /usr/bin/env ruby

require 'rhload';

$version = 'v1';
$bin = File.dirname(File.absolute_path(__FILE__));
$toolhome = File.join(File.dirname($bin),'ruby');
$lib = "lib_#{$version}";
$LOAD_PATH << File.join($toolhome,$lib);

rhload "mainentry.rb";

# debug=Debugger.new(false);
entry = MainEntry.new();
$SIG = entry.run();
debug.print("program exists with sig: #{$SIG}");
exit $SIG;
